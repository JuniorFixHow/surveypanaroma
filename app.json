{
  "expo": {
    "scheme": "acme",
    "userInterfaceStyle": "automatic",
    "orientation": "default",
    "web": {
      "output": "static"
    },
    "name": "surveygh",
    "slug": "surveygh",
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "android": {
      "permissions": ["ACCESS_FINE_LOCATION"]
    },
    "ios": {
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "We need your location to show your position on the map"
      }
    }
  }
}
